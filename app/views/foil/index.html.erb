	    <i class="fas fa-star"></i>
	    <% @feeling = @feeling + 1 %>
	  <%end%>
	  <% @feeling = 0 %>
      </td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Tailles disponibles</th>
<% @options.each do |t, index| %>
<% if t.range != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
      <td><%= t.range %></td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Bridage</th>
<% @options.each do |t, index| %>
<% if t.bridle != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
      <td><%= t.bridle %></td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Prix</th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>
      <td><%= t.price %> €</td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row"></th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>
      <td><a href="<%= kite_path(t) %>" class="scrollup btn btn-success">Détails</a>
</td>
<%end%>
<%end%>
    </tr>

  </tbody>
</table>

