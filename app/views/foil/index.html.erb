          <br>

<table class="table table-striped text-center p-2">
  <thead>
    <tr>
      <th scope="col">kite <i class="fas fa-chevron-circle-right"></i></th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>

      <th scope="col"><%= t.name %></th>
<%end%>
<%end%>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">Compétences<br><i class="fas fa-chevron-circle-down"></i></th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>
      <td><%= link_to kite_path(t), class:"nav-link" do %>
		<% if t.profil.attached? %>
  		  <%= image_tag t.profil, alt: 'kite picture', class:"img-raised rounded img-fluid", style:"width:100px" %>
		<% else %>
		<img src="assets/image.jpg" class="img-raised rounded img-fluid" alt="..." style="width:100px">
		<%end%>
		<%end%>
	</td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Marque</th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>
      <td><%= t.brand %></td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">taille testé</th>
<% @products.each do |t, index| %>
<% if t.category === "kite" && t.domain === "freestyle" %>
      <td><%= t.testsize %>m</td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Wave</th>
<% @technics.each do |t, index| %>
<% if t.wave != nil && @products.find(t.product_id).category === "kite"  && @products.find(t.product_id).domain === "freestyle" %>
      <td><% while @wave < t.wave %>
	    <i class="fas fa-star"></i>
	    <% @wave = @wave + 1 %>
	  <%end%>
	  <% @wave = 0 %>
      </td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Freeride</th>
<% @technics.each do |t, index| %>
<% if t.freeride != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
      <td><% while @freeride < t.freeride %>
	    <i class="fas fa-star"></i>
	    <% @freeride = @freeride + 1 %>
	  <%end%>
	  <% @freeride = 0 %>
      </td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Freestyle</th>
<% @technics.each do |t, index| %>
<% if t.freestyle != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
      <td><% while @freestyle < t.freestyle %>
	    <i class="fas fa-star"></i>
	    <% @freestyle = @freestyle + 1 %>
	  <%end%>
	  <% @freestyle = 0 %>
      </td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Maniability</th>
<% @technics.each do |t, index| %>
<% if t.maniability != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
      <td><% while @maniability < t.maniability %>
	    <i class="fas fa-star"></i>
	    <% @maniability = @maniability + 1 %>
	  <%end%>
	  <% @maniability = 0 %>
      </td>
<%end%>
<%end%>
    </tr>
    <tr>
      <th scope="row">Feeling</th>
<% @technics.each do |t, index| %>
<% if t.feeling != nil && @products.find(t.product_id).category === "kite" && @products.find(t.product_id).domain === "freestyle" %>
